{
  "name": "@duckai/agents",
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "start": "dotenv -e .env -- tsx src/index.ts",
    "monitor": "dotenv -e .env -- tsx src/monitor.ts",
    "typecheck": "tsc --noEmit",
    "deploy:fly": "fly deploy --ha=false",
    "logs": "fly logs",
    "status": "fly status",
    "proto:gen": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=./sdk/src/proto --ts_proto_opt=esModuleInterop=true ./proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.7.10",
    "@types/express": "^5.0.0",
    "axios": "^1.7.9",
    "dotenv": "^16.4.7",
    "dotenv-cli": "^8.0.0",
    "express": "^4.21.2",
    "long": "^5.2.4",
    "openai": "^4.77.0",
    "protobufjs": "^7.4.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "@types/google-protobuf": "^3.15.12",
    "@types/node": "^20.11.16",
    "ts-proto": "^1.165.1",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  },
  "packageManager": "pnpm@8.15.1",
  "engines": {
    "node": ">=18"
  }
}

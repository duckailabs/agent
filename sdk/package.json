{
  "name": "@duckai/agents-sdk",
  "version": "0.1.0",
  "type": "module",
  "main": "src/index.js",
  "files": [
    "src",
    "scripts",
    "p2p-node.js",
    "proto"
  ],
  "scripts": {
    "postinstall": "node scripts/download-p2p.js && pnpm run proto:gen",
    "update-p2p": "P2P_VERSION=0.1.6 node scripts/download-p2p.js && npm run proto:gen",
    "build": "tsc",
    "proto:gen": "protoc --plugin=protoc-gen-ts_proto=./node_modules/.bin/protoc-gen-ts_proto --ts_proto_out=. --ts_proto_opt=esModuleInterop=true ./dist/proto/*.proto"
  },
  "dependencies": {
    "@grpc/grpc-js": "^1.9.13",
    "@grpc/proto-loader": "^0.7.10",
    "long": "^5.2.4",
    "protobufjs": "^7.4.0",
    "rxjs": "^7.8.1"
  },
  "devDependencies": {
    "ts-proto": "^1.181.2"
  },
  "p2pNodeVersion": "0.1.6"
}
